<?xml version="1.0" encoding="UTF-8"?>

<commandSetRoot xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation='commandSet.xsd'>
    <commandSet type="serial">
        <name>hisat2</name>
		<command>
            <type>RunUnixCommand</type>
            <name>hisat2</name>
            <state>incomplete</state>
            <executable>$;BIN_DIR$;/hisat2</executable>            
            <param>
                <key>--seq1file</key>
                <value>$;INPUT_FILE_1$;</value>
            </param>
            <param>
                <key>--seq2file</key>
                <value>'$;INPUT_FILE_2$;'</value>
            </param>
            <param>
                <key>--hisat2_index_dir</key>
                <value>$;HISAT2_INDEX_DIR$;</value>
            </param>
            <param>
                <key>--prefix</key>
                <value>$;HISAT2_INDEX_PREFIX$;</value>
            </param>
            <param>
                <key>--outdir</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;</value>
            </param>
            <param>
                <key>--hisat2_bin_dir</key>
                <value>$;HISAT2_BIN_DIR$;</value>
            </param>
            <param>
                <key>--samtools_bin_dir</key>
                <value>$;SAMTOOLS_BIN_DIR$;</value>
            </param>
            <param>
                <key>--mismatch-penalties</key>
                <value>$;MISMATCH_PENALTIES$;</value>
	        </param>
	        <param>
                <key>--softclip-penalties</key>
                <value>$;SOFTCLIP_PENALTIES$;</value>
	        </param>
	        <param>
                <key>--read-gap-penalties</key>
                <value>$;READ_GAP_PENALTIES$;</value>
	        </param>
	        <param>
                <key>--ref-gap-penalties</key>
                <value>$;REF_GAP_PENALTIES$;</value>
	        </param>
	        <param>
                <key>--min-intronlen</key>
                <value>$;MIN_INTRON_LENGTH$;</value>
	        </param>
	        <param>
                <key>--max-intronlen</key>
                <value>$;MAX_INTRON_LENGTH$;</value>
	        </param>
	        <param>
                <key>--score-min</key>
                <value>$;MIN_ALIGNMENT_SCORE$;</value>
	        </param>
	        <param>
                <key>--pen-cansplice</key>
                <value>$;PEN_CANSPLICE$;</value>
	        </param>
	        <param>
                <key>--num-threads</key>
                <value>$;NUMBER_THREADS$;</value>
	        </param>
	        <param>
                <key>--pen-noncansplice</key>
                <value>$;PEN_NONCANSPLICE$;</value>
	        </param>
	        <param>
                <key>--pen-canintronlen</key>
                <value>$;PEN_CANINTRONLEN$;</value>
	        </param>
	        <param>
                <key>--pen-noncanintronlen</key>
                <value>$;PEN_NONCANINTRONLEN$;</value>
	        </param>
	        <param>
                <key>--rna-strandness</key>
                <value>$;RNA_STRANDNESS$;</value>
	        </param>
	        <param>
                <key>--dta-cufflinks</key>
                <value>$;DTA_CUFFLINKS$;</value>
	        </param>
	        <param>
                <key>--num-alignments</key>
                <value>$;NUM_ALIGNMENTS$;</value>
	        </param>
	        <param>
                <key>--minins</key>
                <value>$;MIN_FRAGMENT_LENGTH$;</value>
	        </param>
	        <param>
                <key>--maxins</key>
                <value>$;MAX_FRAGMENT_LENGTH$;</value>
	        </param>
	        <param>
                <key>--no-unal</key>
                <value>$;SUPPRESS_UNALIGNMENTS$;</value>
	        </param>
	        <param>
                <key>--args</key>
                <value>'$;OTHER_PARAMETERS$;'</value>
            </param>
            <param>
                <key>stdout</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/hisat2.stdout</value>
            </param>
            <param>
                <key>stderr</key>
                <value>$;OUTPUT_DIRECTORY$;/$;ITERATOR_NAME$;/g$;GROUP_NUMBER$;/hisat2.stderr</value>
            </param>
            <arg>$;OTHER_ARGS$;</arg>
        </command>
    </commandSet>
</commandSetRoot>
