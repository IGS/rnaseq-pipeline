#!/bin/sh
export LD_LIBRARY_PATH=/usr/local/packages/python-3.8.2/lib:$LD_LIBRARY_PATH
export PATH=/usr/local/packages/python-3.8.2/bin:/usr/local/packages/sge-root/bin/lx24-amd64:/usr/local/bin:$PATH
if [ -z "$SCHEMA_DOCS_DIR" ]
then
    SCHEMA_DOCS_DIR=
export SCHEMA_DOCS_DIR
fi
if [ -z "$WORKFLOW_WRAPPERS_DIR" ]
then
    WORKFLOW_WRAPPERS_DIR=/usr/local/projects/ergatis/package-Rseqv2b3/bin
export WORKFLOW_WRAPPERS_DIR
fi
if [ -z "$WORKFLOW_DOCS_DIR" ]
then
    WORKFLOW_DOCS_DIR=
export WORKFLOW_DOCS_DIR
fi


umask 0000

unset PERL5LIB
unset LD_LIBRARY_PATH

export LD_LIBRARY_PATH=/usr/local/packages/tbb/lib/intel64/gcc4.7

LANG=C
export LANG
LC_ALL=C
export LC_ALL

PERL_MOD_DIR=/usr/local/projects/ergatis/package-Rseqv2b3/lib/5.8.8
export PERL_MOD_DIR

export PERL5LIB=/usr/local/projects/ergatis/package-Rseqv2b3/lib/perl5/

    /usr/local/bin/perl /usr/local/projects/ergatis/package-Rseqv2b3/bin/bowtie.pl "$@"    

